  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.7.21",
    "concurrently": "^7.4.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "express": "^4.18.1",
    "socket.io": "^4.5.2"
  }
}
